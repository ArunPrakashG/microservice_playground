{
  "id": null,
  "title": "Service-A Canary Overview",
  "uid": "service-a-canary",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "browser",
  "style": "dark",
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Throughput (req/s)",
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"service-a\", handler=\"/\"}[1m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "descending"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Latency P95 (ms)",
      "id": 2,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"service-a\", handler=\"/\"}[1m])) by (le, instance)) * 1000",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "id": 3,
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{job=\"service-a\", status!~\"2..\", handler=\"/\"}[5m])) by (instance) / sum(rate(http_requests_total{job=\"service-a\", handler=\"/\"}[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ],
  "templating": {
    "list": []
  }
}
